{
  "name": "@aircomponents/ui",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "description": "custom components",
  "main": "dist/index.cjs.js",
  "module": "dist/aircomponents/aircomponents.esm.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
